import java.util.stream.Collectors;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String str="123abcd@$";
		
	str.chars().filter(Character::isDigit)
					.mapToObj(c->(char)c)
					.collect(Collectors.toList()).forEach(System.out::println);
		

	}

}
