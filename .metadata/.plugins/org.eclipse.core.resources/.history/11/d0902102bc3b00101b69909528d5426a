
public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str= "aa3gj51jkl6kk7";
		//Given string how write the sum of integers using stream
		
		Integer result=str.chars().filter(c->Character.isDigit(c)).map(c->c).sum();
		//Integer result=str.chars().filter(c->Character.isDigit(c)).map(c->Character.getNumericValue(c)).sum();
		System.out.println(result);
	}

}
